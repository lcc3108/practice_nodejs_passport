{
  "name": "test",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/index.js",
    "prebuild": "rm -rf dist index.zip",
    "build": "NODE_ENV=production node -r ./env.js ./node_modules/.bin/babel src --out-dir dist --copy-files --extensions \".ts,.tsx\"",
    "postbuild": "cp -t ./dist package.json google_key.json",
    "test": "NODE_ENV=test mocha -r ./mocha.js --timeout 10000",
    "test:all": "NODE_ENV=test mocha -r ./mocha.js --timeout 10000 ./src/tests/**/*.spec.ts",
    "test:local": "NODE_ENV=test node -r ./mocha.js ./src/tests/express.ts",
    "test:prod": "NODE_ENV=production node -r ./mocha.js ./src/tests/express.ts",
    "test:prod:all": "NODE_ENV=production mocha -r ./mocha.js --timeout 10000 ./src/tests/**/*.spec.ts",
    "test:local:watch": "NODE_ENV=test nodemon -r ./mocha.js ./src/tests/express.ts --watch ./src -e ts",
    "lint": "tslint ./src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@google-cloud/firestore": "^2.2.6",
    "apollo-server-express": "^2.8.2",
    "await-to-js": "^2.1.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "graphql": "^14.5.0",
    "graphql-constraint-directive": "^1.4.0",
    "ioredis": "^4.14.0",
    "ioredis-mock": "^4.16.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "source-map-support": "^0.5.13",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.5.5",
    "@types/bluebird": "^3.5.27",
    "@types/dotenv": "^6.1.1",
    "@types/express-jwt": "^0.0.42",
    "@types/ioredis": "^4.0.15",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.2",
    "@types/redis": "^2.8.13",
    "@types/validator": "^10.11.3",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-preset-minify": "^0.5.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cookie-parser": "^1.4.4",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "tslint": "^5.19.0",
    "typescript": "^3.5.3"
  }
}
