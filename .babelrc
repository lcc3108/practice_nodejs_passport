{
  "presets": ["@babel/typescript", ["@babel/env", { "targets": { "node": "8" } }]],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": { "@": "./src" }
      },
    ],
    "transform-inline-environment-variables"
  ],
  "sourceMaps": true,
  "comments": false
}
