  "scripts": {
    "install:backend": "yarn install --prefix=backend",
    "install:frontend": "yarn install --prefix=frontend",
    "preinstall:all": "yarn install:backend && yarn install:frontend",
    "build:backend": "yarn build --prefix=backend",
    "build:frontend": "yarn build --prefix=frontend",
