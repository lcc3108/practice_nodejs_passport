{
    "name": "test",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "node ./dist/index.js",
        "prebuild": "rm -rf dist index.zip",
        "build": "babel src --out-dir dist --copy-files --extensions \".ts,.tsx\"",
        "postbuild": "cd dist && bestzip ../index.zip ./index.js",
        "predeploy": "yarn build",
        "deploy": "terraform plan && terraform apply --auto-approve",
        "lint": "tslint ./src/**/*.ts"
    },
    "dependencies": {},
    "devDependencies": {
        "@babel/cli": "^7.5.5",
        "@babel/core": "^7.5.5",
        "@babel/preset-env": "^7.5.5",
        "@babel/preset-typescript": "^7.3.3",
        "babel-loader": "^8.0.6",
        "bestzip": "^2.1.4",
        "tslint": "^5.18.0",
        "typescript": "^3.5.3",
        "webpack": "^4.39.1",
        "webpack-cli": "^3.3.6"
    }
}
